  },
  "homepage": "https://github.com/birdofpreyru/react-helmet#readme",
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "invariant": "^2.2.4",
    "react-fast-compare": "^3.2.2",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/eslint-plugin": "^7.25.9",
