  "description": "Thread-safe Helmet for React 16+ and friends",
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.esm.js",
    "default": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && NODE_ENV=production tsx build.ts && npm run types",
    "lint": "eslint",
    "test": "npm run lint && npm run typecheck && npm run jest",
    "typecheck": "tsc",
    "jest": "jest --config config/jest/config.js"
  },
  "repository": {
    "type": "github",
    "url": "git+https://github.com/birdofpreyru/react-helmet.git"
  },
  "keywords": [
    "helmet",
